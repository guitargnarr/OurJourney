# Backend Environment Variables for Railway
# Copy this to .env in production or set in Railway dashboard

# Database (Railway will provide this automatically)
DATABASE_URL=postgresql://user:password@host:port/database

# Authentication - CHANGE THIS!
AUTH_PASSWORD=your_secure_password_here

# CORS - Add your Vercel frontend URL
CORS_ORIGIN=https://your-app.vercel.app,https://ourjourney-e3bahua8r-matthew-scotts-projects-1dc9743e.vercel.app

# Environment
NODE_ENV=production

# Port (Railway auto-assigns via $PORT environment variable)
PORT=3001
